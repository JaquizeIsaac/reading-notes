        console.log("Question 3: User's response - " + response3);

        // Question 4
        var response4 = prompt("Tell me something interesting about yourself.");
        console.log("Question 4: User's response - " + response4);

        // Display concatenated response in an alert
        var concatenatedResponse = "Thank you for your time. " + response4 + ", I have two cats myself, Lazer and Greyson. I would love to travel to " + response2 + " and my name is M.J. Visit again"
